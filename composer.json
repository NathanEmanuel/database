{
    "name": "compucie/database",
    "description": "Library of database managers used by Newton's systems.",
    "version": "5.0.3",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Compucie\\Database\\": "src/"
        },
        "files": [
            "src/utils.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Compucie\\DatabaseTest\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^11.2"
    },
    "require": {
        "ext-mysqli": "*"
    },
    "scripts": {
        "test": "vendor/bin/phpunit --fail-on-empty-test-suite --display-errors --display-notices --display-warnings --display-all-issues --teamcity"
    }
}
